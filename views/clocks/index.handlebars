<table class="table table-striped">
  <tbody>
    <td><a href="/clocks/new" class="btn btn-success">New</a></td>
    {{#each clocks}}
    <tr>
      <td>{{id}}</td>
      <td>{{manufacturer}}</td>
      <td>{{model}}</td>
      <td>{{type}}</td>
      <td><a href="/clocks/{{id}}" class="btn btn-primary">Show</a></td>
      <td><a href="/clocks/{{id}}/edit" class="btn btn-secondary">Edit</a></td>
      <td>
        <form method="POST" action="/clocks/{{id}}?_method=DELETE">
          <input class="btn btn-danger" type="submit" value="Delete"/>
        </form>
      </td>
    </tr>
    {{/each}}    
  </tbody>
</table>